                                      "Value": "$$Sub InsertPicture()\r\n    Dim oSlide As Slide ' 当前幻灯片对象\r\n    Dim oPic As Shape ' 插入的图片对象\r\n    Dim sFilePath As String ' 图片路径\r\n    Dim dSize As Double ' 图片的目标高度（以厘米为单位）\r\n\r\n    ' === 1. 设置图片路径和图片高度（可自行修改路径和高度） ===\r\n    sFilePath = \"{fullPath}\" ' 在此处设置图片路径\r\n    dSize = \"7\" * 28.34567 ' 将厘米转换为磅 (1厘米 = 28.34567磅)\r\n\r\n    ' === 2. 检查文件是否存在 ===\r\n    If Len(Dir(sFilePath)) = 0 Then\r\n        MsgBox \"文件不存在，请检查文件路径\", vbExclamation\r\n        Exit Sub\r\n    End If\r\n\r\n    ' === 3. 获取当前活动的幻灯片 ===\r\n    On Error Resume Next\r\n    Set oSlide = ActivePresentation.Slides(ActiveWindow.View.Slide.SlideIndex)\r\n    On Error GoTo 0\r\n\r\n    If oSlide Is Nothing Then\r\n        MsgBox \"没有找到活动的幻灯片\", vbExclamation\r\n        Exit Sub\r\n    End If\r\n\r\n    ' === 4. 插入图片，并设置图片大小和位置 ===\r\n    Set oPic = oSlide.Shapes.AddPicture(FileName:=sFilePath, LinkToFile:=msoFalse, SaveWithDocument:=msoTrue, Left:=0, Top:=0, Width:=-1, Height:=-1)\r\n    With oPic\r\n        ' 锁定图片的纵横比，防止变形\r\n        .LockAspectRatio = msoTrue\r\n        ' 设置图片的高度（根据dSize），宽度会根据锁定的比例自动调整\r\n        .Height = dSize\r\n        ' 将图片位置调整为居中\r\n        .Left = (ActivePresentation.PageSetup.SlideWidth - .Width) / 2\r\n        .Top = (ActivePresentation.PageSetup.SlideHeight - .Height) / 2\r\n        ' 选中图片\r\n        .Select\r\n    End With\r\n\r\n    ' === 5. 释放对象引用，防止内存泄露 ===\r\n    Set oSlide = Nothing\r\n    Set oPic = Nothing\r\nEnd Sub\r\n"
                                    },
